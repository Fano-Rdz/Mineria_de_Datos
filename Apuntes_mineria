    {
      "cell_type": "code",
      "source": [
        "# Tablas cruzadas (NumPy)\n",
        "def tabla2x2(a, b):\n",
        "    t = np.zeros((2,2), dtype=int)\n",
        "    t[1,1] = np.count_nonzero(a & b)\n",
        "    t[1,0] = np.count_nonzero(a & ~b)\n",
        "    t[0,1] = np.count_nonzero(~a & b)\n",
        "    t[0,0] = np.count_nonzero(~a & ~b)\n",
        "    return t\n",
        "\n",
        "tab_py_colab = tabla2x2(prog_py, usa_colab)\n",
        "tab_py_gh    = tabla2x2(prog_py, cuenta_gh)\n",
        "tab_colab_gh = tabla2x2(usa_colab, cuenta_gh)"
      ],
      "metadata": {
        "id": "KWMjlE--dNcG"
      },
      "execution_count": 22,
      "outputs": []
    },
